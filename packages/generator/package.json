{
  "name": "prisma-next-crud-generator",
  "description": "Prisma generator for scaffolding application based on models",
  "version": "1.0.1",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-next-crud-generator": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "4.9.0",
    "@prisma/generator-helper": "4.9.0",
    "@prisma/sdk": "^3.15.2",
    "prettier": "2.8.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "18.13.0",
    "@types/prettier": "2.7.2",
    "jest": "29.4.1",
    "prisma": "4.10.0",
    "semantic-release": "^20.1.0",
    "ts-jest": "29.0.5",
    "typescript": "4.9.4"
  },
  "homepage": "https://github.com/MBrunoS/prisma-next-crud-generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/MBrunoS/prisma-next-crud-generator"
  },
  "author": "Maur√≠cio Bruno <contact@mbrunos.dev>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator",
    "nextjs",
    "crud",
    "scaffold"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
