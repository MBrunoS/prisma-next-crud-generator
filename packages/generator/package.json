{
  "name": "prisma-next-crud-generator",
  "description": "Generate CRUD operations for Next.js from Prisma schema",
  "version": "1.3.0",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-next-crud-generator": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "vitest"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "@prisma/generator-helper": "^5.19.1",
    "@prisma/internals": "^5.19.1",
    "pluralize": "^8.0.0",
    "prettier": "3.3.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "22.5.5",
    "@types/pluralize": "^0.0.33",
    "prisma": "^5.19.1",
    "semantic-release": "^24.1.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "homepage": "https://github.com/MBrunoS/prisma-next-crud-generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/MBrunoS/prisma-next-crud-generator"
  },
  "author": "Maur√≠cio Bruno <contact@mbrunos.dev>",
  "keywords": [
    "prisma",
    "generator",
    "nextjs",
    "next",
    "crud",
    "crud-generator",
    "scaffold",
    "scaffolding"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
