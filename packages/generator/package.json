{
  "name": "prisma-next-crud-generator",
  "description": "Prisma generator for scaffolding application based on models",
  "version": "1.0.0",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-next-crud-generator": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "3.12.0",
    "@prisma/generator-helper": "3.12.0",
    "@prisma/sdk": "3.12.0",
    "prettier": "2.5.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "27.0.3",
    "@types/node": "17.0.21",
    "@types/prettier": "2.4.2",
    "jest": "27.4.7",
    "memfs": "^3.4.13",
    "prisma": "3.12.0",
    "semantic-release": "^20.1.0",
    "ts-jest": "27.1.4",
    "typescript": "4.6.2"
  },
  "homepage": "https://github.com/MBrunoS/prisma-next-crud-generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/MBrunoS/prisma-next-crud-generator"
  },
  "author": "Maur√≠cio Bruno <contact@mbrunos.dev>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator",
    "nextjs",
    "crud",
    "scaffold"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
